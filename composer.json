{
    "name": "mcesar/laravel-survey",
    "description": "Survey package for Laravel 5.5 and up",
    "keywords": [
        "laravel",
        "survey",
        "question",
        "answer"
    ],
    "homepage": "https://github.com/spatie/laravel-permission",
    "license": "MIT",
    "authors": [
        {
            "name": "Milo Cesar",
            "email": "me@mcesar.eu"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/config": "5.7.*",
        "illuminate/console": "5.7.*",
        "illuminate/database": "5.7.*",
        "illuminate/routing": "5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MCesar\\Survey\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MCesar\\Survey\\Test\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MCesar\\Survey\\SurveyServiceProvider"
            ],
            "aliases": {
                "Category": "MCesar\\Survey\\Facade\\Category"
            }
        }
    }
}
