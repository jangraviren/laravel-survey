{
    "name": "jangraviren/laravel-survey",
    "description": "Original credits goes to mceaser/laravel-survey. Only add 5.7.* support",
    "keywords": [
        "laravel",
        "survey",
        "question",
        "answer"
    ],
    "homepage": "https://github.com/spatie/laravel-permission",
    "license": "MIT",
    "authors": [
        {
            "name": "Virender Kumar",
            "email": "jangra.viren@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0|~5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "MCesar\\Survey\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MCesar\\Survey\\Test\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MCesar\\Survey\\SurveyServiceProvider"
            ]
        }
    }
}
